-claims.each do |claim|
  %tr
    %td.defendants
      = f.label(caseworker_claim_id_helper(claim)) do
        #{claim.defendants.map(&:name).join(", ")}
    %td.case_number
      = f.label(caseworker_claim_id_helper(claim)) do
        = claim.case_number
    %td.advocate
      = f.label(caseworker_claim_id_helper(claim)) do
        = claim.advocate.email
    %td.court_name
      = f.label(caseworker_claim_id_helper(claim)) do
        = claim.court.name
    %td
      = claim_allocation_checkbox_helper(claim, case_worker)