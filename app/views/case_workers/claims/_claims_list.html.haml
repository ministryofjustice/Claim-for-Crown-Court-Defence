#report-options.container
  = render partial: 'search_form', locals: { archived: params[:action] == 'archived' ? true : false }

#search-summary
  - if params[:search].present?
    = render partial: 'shared/search_summary'

#list-controls
  = render partial: 'list_controls'

- if @claims.none?
  %p
    No claims found
- else
  = render partial: 'claims'