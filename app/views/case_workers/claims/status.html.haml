= content_for :page_title, flush: true do
  = t('.page_title')

- present(@claim) do |claim|
  = render 'errors/error_summary', ep: @claim, error_summary: t('.error_summary')
  = render 'errors/injection_error_summary', ep: claim
  = render 'warnings/injection_warnings_summary', claim: claim
  = render partial: 'shared/claim_header', locals: { claim: claim }  # Here we are st
  = render partial: 'shared/case_summary', locals: { claim: claim }
  = render partial: 'shared/claim_history', locals: { claim: claim }
  = render partial: 'shared/claim_status', locals: { claim: claim, status_disabled: false }
