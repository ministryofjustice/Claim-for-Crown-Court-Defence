= render partial: 'error_summary', locals: { ep: @error_presenter }

= render partial: 'layouts/header', locals: {page_heading: t('external_users.start_agfs_claim_heading')}

- present(@claim) do |claim|
  .grid-row
    .column-two-thirds
      = render partial: 'form', locals: { claim: claim }

    .column-one-third
      = render partial: 'external_users/claims/summary_claims_sidebar', locals: { claim: claim, display_buttons: false }
