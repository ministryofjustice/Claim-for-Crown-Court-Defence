$('#importer').find('form')
  .find('.file-upload-name')
    .empty();

$('#importer')
  .addClass('has-errors')
  .find('.errors')
    .html("<%= html_escape_once(errors[:file].to_sentence) %>");

$('.importer-results').empty();
$('input.btn-import-file').prop('disabled', false).val('Import claims').prop('aria-hidden', false).hide();
