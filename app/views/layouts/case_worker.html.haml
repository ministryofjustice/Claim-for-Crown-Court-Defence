- @disable_logo_link  = true
- content_for :head do
  %meta{:name =>"format-detection", :content => "telephone=no"}/
  = stylesheet_link_tag "case_workers/application", media: "all"

- content_for :body_end do
  = javascript_include_tag "application"

= content_for :content do
  = csrf_meta_tags

  .outer-block
    #primary-nav-bar.internal-user
      - if user_signed_in?
        = render partial: 'layouts/primary_navigation'
    .phase-banner
      %p
        %strong.phase-tag= config_item(:phase)
        %span
          This is a new service â€“ your
          %a{href: "#"} feedback
          will help us to improve it.
    .container
      = render partial: 'layouts/flashes' unless flash.empty?
      %main.main-content
        = yield

= render template: 'layouts/moj_internal_template'
